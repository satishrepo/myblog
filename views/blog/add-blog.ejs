<% include ../header %>



    <div class="container">
    	<form method="POST" action="/blog/save" enctype="multipart/form-data">
		  <div class="form-group">
		    <label for="formGroupExampleInput">Title</label>
		    <input type="text" class="form-control"  placeholder="Title" name="title">
		  </div>
		  <div class="form-group">
		    <label for="formGroupExampleInput">Image</label>
		    <input type="file" class="form-control"  placeholder="Post Picture" name="postpic">
		  </div>
		  <div class="form-group">
		    <label for="formGroupExampleInput2">Content</label>
		    <textarea class="form-control" placeholder="Content" name='content' rows="5"></textarea>
		  </div>
		  
		  <button type="submit" class="btn btn-primary">Submit</button>
		</form>
    </div>
